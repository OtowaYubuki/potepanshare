<div id="room_new">
 <h2>ルーム情報登録</h2>
  <%= form_with model: @room do |f| %>
   <table>
     <tr>
       <th><%= f.label :name ,"ルーム名" %></th>
       <td><%= f.text_field :name %></td>
     </tr>
 
     <tr>
       <th><%= f.label :introduction ,"ルーム紹介" %></th>
       <td><%= f.text_area :introduction %></td>
     </tr>
 
     <tr>
       <th><%= f.label :price ,"料金" %></th>
       <td><%= f.text_field :price %></td>
     </tr>
 
     <tr>
       <th><%= f.label :adress ,"住所" %></th>
       <td><%= f.text_field :adress %></td>
     </tr>

     <tr>
       <th><%= f.label :image ,"ルーム画像" %></th>
       <td><%= f.file_field :image %></td>
     </tr>


   </table>
   <div>
     <ul>
       <li><%= f.submit "登録"%></li>
     </ul>
   </div>
 <% end %>